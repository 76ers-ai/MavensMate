<?xml version="1.0" encoding="UTF-8"?>
<{{ xmlName }} xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>{{ apiVersion }}</apiVersion>
{% if xmlName === 'ApexPage' or xmlName === 'ApexComponent' %}
	<label>{{ apiName }}</label>
	<description></description>
{% endif %}
{% if xmlName === 'ApexTrigger' %}
	<status>Active</status>
{% endif %}
</{{ xmlName }}>
